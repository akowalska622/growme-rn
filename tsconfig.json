{
  "extends": "expo/tsconfig.base",
  "typeRoots": ["./src/types"],
  "compilerOptions": {
    "strict": true,
    "baseUrl": "./src",
    "paths": {
      "Config/*": ["./config/*"],
      "Constants/*": ["./constants/*"],
      "Enums/*": ["./enums/*"],
      "Hooks/*": ["./hooks/*"],
      "Redux/*": ["./redux/*"],
      "Screens/*": ["./screens/*"],
      "UI/*": ["./ui/*"]
    }
  }
}
